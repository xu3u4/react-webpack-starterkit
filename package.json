{
  "name": "react-webpack-starterkit",
  "version": "0.2.0",
  "description": "A quick start for react+webpack, which includes basic packages and fundamental settings.",
  "main": "index.html",
  "scripts": {
    "production": "webpack --config config/webpack.config.prod.js",
    "dev": "webpack-dev-server --config config/webpack.config.dev.js --progress --color",
    "eslint": "eslint src",
    "eslint:fix": "eslint src --fix",
    "test:watch": "jest test --watch",
    "test": "jest test"
  },
  "sideEffects": false,
  "jest": {
    "modulePaths": [
      "src"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|scss)$": "identity-obj-proxy"
    },
    "testEnvironment": "jsdom"
  },
  "repository": {
    "type": "git",
    "url": "react-webpack-starterkit"
  },
  "keywords": [
    "react"
  ],
  "author": "Jocelyn",
  "license": "MIT",
  "dependencies": {
    "prop-types": "^15.8.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.4",
    "@types/react": "^17.0.41",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^27.5.1",
    "babel-loader": "^8.2.3",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^6.7.1",
    "eslint": "^8.11.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-import-resolver-webpack": "^0.13.2",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.3.0",
    "html-webpack-plugin": "^5.5.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.5.1",
    "node-sass": "^7.0.1",
    "sass-loader": "^12.6.0",
    "style-loader": "^3.3.1",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4"
  }
}
